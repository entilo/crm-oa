<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
	<!-- Mybatis generator版本：1.3.5 -->

	<context id="test1">
		<!-- 去掉自动Mybatis generator生成的注释 -->
		<commentGenerator>
			<property name="suppressDate" value="true" />
			<property name="suppressAllComments" value="true" />
		</commentGenerator>

		<!-- JDBC CONN -->
		<jdbcConnection connectionURL="jdbc:mysql://127.0.0.1:3306/crm"
			driverClass="com.mysql.jdbc.Driver" password="entilo" userId="root" />

		<!-- entity/pojo/domain -->
		<javaModelGenerator targetPackage="com.h.www.entity"
			targetProject="house" />

		<!-- maven项目sqlMapGenerator配置 Mapper.xml -->
		<!-- <sqlMapGenerator targetPackage="com.ct.ic.st.eu.dao" targetProject="cms-core/src/main/java"> -->
		<!-- Mapper.xml -->
		<sqlMapGenerator targetPackage="com.h.www.dao.impl"
			targetProject="house" />

		<!-- dao/Mapper -->
		<javaClientGenerator targetPackage="com.h.www.dao"
			targetProject="house" type="XMLMAPPER" />


		<table schema="root" tableName="%" enableCountByExample="false"
			enableUpdateByExample="false" enableDeleteByExample="false"
			enableSelectByExample="false" selectByExampleQueryId="false">
		</table>
		
		
		
	</context>
</generatorConfiguration>