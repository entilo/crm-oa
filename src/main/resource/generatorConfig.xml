<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
	<!-- Mybatis generator版本：1.3.5 -->

	<context id="test1">
		<!-- 去掉自动Mybatis generator生成的注释 -->
		<commentGenerator>
			<property name="suppressDate" value="true" />
			<property name="suppressAllComments" value="true" />
		</commentGenerator>

		<!-- JDBC CONN -->
		<jdbcConnection connectionURL="jdbc:mysql://127.0.0.1:3306/crm"
			driverClass="com.mysql.jdbc.Driver" password="entilo" userId="root" />

		<!-- entity/pojo/domain -->
		<javaModelGenerator targetPackage="com.h.www.entity"
			targetProject="house" />

		<!-- maven项目sqlMapGenerator配置 Mapper.xml -->
		<!-- <sqlMapGenerator targetPackage="com.ct.ic.st.eu.dao" targetProject="cms-core/src/main/java"> -->
		<!-- Mapper.xml -->
		<sqlMapGenerator targetPackage="com.h.www.dao.impl"
			targetProject="house" />

		<!-- dao/Mapper -->
		<javaClientGenerator targetPackage="com.h.www.dao"
			targetProject="house" type="XMLMAPPER" />


		<!-- SELECT uid, username, `password`, phone, sex FROM t_admin -->
		<!-- SELECT id, gid,group_name, uid, function_id FROM `t_group` -->
		<!-- SELECT id, fid, furl FROM `t_function` -->
		<!-- SELECT houseid, housezorn, housebuilding,houselevel, houseroom, housesize, 
			ishouseenpty FROM t_house -->
		<!-- SELECT uid, uname, uindentyid, uborndate, uillness,user_describ, houseid, 
			ishouse_owner FROM `t_user` -->
		<!-- SELECT id, houseid, debt FROM t_housedept -->

		<!-- 需要映射的表和映射规则 -->
		<!-- schema可随意填，但不能不填 -->
		<!--默认声称所有的表，而且用驼峰命名方式转换数据库中的表或者结构：如"t_user"："TUser" 列的话是"t_username":"tName"
		<table schema="root" tableName="%" enableCountByExample="false"
			enableUpdateByExample="false" enableDeleteByExample="false"
			enableSelectByExample="false" selectByExampleQueryId="false">
		</table>
		-->
		
		<table schema="root" tableName="t_housedept" domainObjectName="HouseDept"
			enableCountByExample="false" enableUpdateByExample="false"
			enableDeleteByExample="false" enableSelectByExample="false"
			selectByExampleQueryId="false">
			<columnOverride column="id" property="depeId" />
			<columnOverride column="houseid" property="hosueId" />
			<columnOverride column="debt" property="houseDept" />
		</table>
		<table schema="root" tableName="t_user" domainObjectName="User"
			enableCountByExample="false" enableUpdateByExample="false"
			enableDeleteByExample="false" enableSelectByExample="false"
			selectByExampleQueryId="false">
			<columnOverride column="uid" property="userId" />
			<columnOverride column="uname" property="username" />
			<columnOverride column="uindentyid" property="userIdentityId" />
			<columnOverride column="uborndate" property="bornDate" />
			<columnOverride column="uillness" property="userIllness" />
			<columnOverride column="user_describ" property="userDescrib" />
			<columnOverride column="houseid" property="houseId" />
			<columnOverride column="ishouse_owner" property="isHouseOwner" />
		</table>
		<table schema="root" tableName="t_house" domainObjectName="House"
			enableCountByExample="false" enableUpdateByExample="false"
			enableDeleteByExample="false" enableSelectByExample="false"
			selectByExampleQueryId="false">
			<columnOverride column="houseid" property="houseId" />
			<columnOverride column="housezorn" property="houseZorn" />
			<columnOverride column="housebuilding" property="houseBuinding" />
			<columnOverride column="houselevel" property="houseLevel" />
			<columnOverride column="houseroomnum" property="houseRoomNum" />
			<columnOverride column="housesize" property="houseSize" />
			<columnOverride column="ishouseenpty" property="isHouseEmpty" />
		</table>
		<table schema="root" tableName="t_function" domainObjectName="Function"
			enableCountByExample="false" enableUpdateByExample="false"
			enableDeleteByExample="false" enableSelectByExample="false"
			selectByExampleQueryId="false">
			<columnOverride column="id" property="id" />
			<columnOverride column="fid" property="groupId" />
			<columnOverride column="furl" property="groupName" />
		</table>
		<table schema="root" tableName="t_group" domainObjectName="Group"
			enableCountByExample="false" enableUpdateByExample="false"
			enableDeleteByExample="false" enableSelectByExample="false"
			selectByExampleQueryId="false">
			<columnOverride column="id" property="id" />
			<columnOverride column="gid" property="groupId" />
			<columnOverride column="group_name" property="groupName" />
			<columnOverride column="uid" property="userId" />
			<columnOverride column="function_id" property="functionId" />
		</table>

		<table schema="root" tableName="t_admin" domainObjectName="Admin"
			enableCountByExample="false" enableUpdateByExample="false"
			enableDeleteByExample="false" enableSelectByExample="false"
			selectByExampleQueryId="false">
			<columnOverride column="uid" property="adminId" />
			<columnOverride column="username" property="username" />
			<columnOverride column="password" property="password" />
			<columnOverride column="phone" property="phone" />
		</table>
	</context>
</generatorConfiguration>